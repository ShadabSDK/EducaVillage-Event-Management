@model IEnumerable<EventManagement.Models.Event>

@{
    ViewBag.Title = "events";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Events</h2>
<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>Movie</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var data in Model)
        {
            <tr>
                <td>@data.Name</td>
            </tr>
        }
    </tbody>
</table>

